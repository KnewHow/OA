<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.itcast.oa.domain.Person" table="person">
		<id name="pid" type="java.lang.Long" length="5">
			<column name="pid"></column>
			<generator class="increment"></generator>
		</id>
		
		<property name="pname" type="java.lang.String" length="10">
			<column name="pname"></column>
		</property>
		
		<property name="psex" type="java.lang.String" length="5">
			<column name="psex"></column>
		</property>
	</class>
</hibernate-mapping>